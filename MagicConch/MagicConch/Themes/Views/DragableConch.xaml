<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:units="clr-namespace:MagicConch.Themes.Units">
    <Style TargetType="units:DragableConch">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="units:DragableConch">
                        <Grid>
                            <Image x:Name="PART_Body" Source="/MagicConch;component/Assets/Images/Conch_Body.png"/>
                            <Image x:Name="PART_Handle" Source="/MagicConch;component/Assets/Images/Conch_Handle.png"/>
                        </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
