<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:units="clr-namespace:MagicConch.Themes.Units">
    <Style TargetType="units:FloattingBubble">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="units:FloattingBubble">
                    <Grid>
                        <Image x:Name="Body" Source="/MagicConch;component/Assets/Images/Conch_Body.png"/>
                        <Image x:Name="Handle" Source="/MagicConch;component/Assets/Images/Conch_Handle.png"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
